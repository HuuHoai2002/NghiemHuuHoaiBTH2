@{
    ViewData["Title"] = "Person";
}
@model IEnumerable<NghiemHuuHoaiBTH2.Models.Person>

<div>
    <a class="btn btn-success" asp-action="Create">Create a new Person</a>
</div>

<table class="table table-all">
    <tr>
        <th>Person ID</th>
        <th>Person Name</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.PersonID</td>
            <td>@item.PersonName</td>
        </tr>
    }
</table>